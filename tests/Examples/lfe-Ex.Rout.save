> ## then the default
> print(ef <- efactory(obj,'ref'))
function (v, addnames) 
{
    esum <- sum(v[extrarefs])
    df <- v[refsubs]
    sub <- ifelse(is.na(df), 0, df)
    df <- v[refsuba]
    add <- ifelse(is.na(df), 0, df + esum)
    v <- v - sub + add
    if (addnames) {
        names(v) <- nm
        attr(v, "extra") <- list(obs = obs, comp = comp, fe = fef, 
            idx = idx)
    }
    v
}
